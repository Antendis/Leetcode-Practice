class Solution(object):
    def numIdenticalPairs(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        count = 0

        for i, firstNum in enumerate(nums):

            for secondNum in nums[i+1:]:
                
                print firstNum, secondNum
                if firstNum == secondNum and nums.index(firstNum) < nums.index(secondNum):
                    print "yes"
                    count += 1

        return count

