class Solution(object):
    def romanToInt(self, s):

        number = 0
        i = 0

        while i+1 < len(s):
            if s[i] == "I":
                if i+1 < len(s) and s[i+1] == "X":
                    number += 9
                    i += 2
                if i+1 < len(s) and s[i+1] == "V":
                    number += 4
                else:
                    number += 1
                    i += 1

            elif s[i] == "X":
                if (s[i+1] == "L"):
                    number += 40
                    i += 2
                if (s[i+1] == "C"):
                    number += 90
                    i += 2
                else:
                    number += 10
                    i += 1

            elif s[i] == "C":
                if (s[i+1] == "D"):
                    number += 400
                    i += 2
                if (s[i+1] == "M"):
                    number += 900
                    i += 2
                else:
                    number += 100
                    i += 1

        return number



