class Solution(object):
    def romanToInt(self, s):
        values = {I:1, V:5, X:10, L:50, C:100, D:500, M:1000}
        number = 0
        n = len(s)
        i = 0

        while i < n:
            if i+1 < n and values[s[i]] < values[s[i+1]]:
                number += values[s[i+1]] - values[s[i]]
                i +=2
            else:
                number += values[s[i]]
                i += 1

        return number
