class Solution(object):
    def romanToInt(self, s):

        number = 0
        i = 0

        while i < len(s):

            if s[i] == "I":
                if (s[i+1] == "X"):
                    number += 4
                    i += 2
                    continue
                if (s[i+1] == "V"):
                    number += 9
                    i += 2
                    continue
                else:
                    number += 1
                    i += 1
                    continue

            elif s[i] == "X":
                if (s[i+1] == "L"):
                    number += 40
                    i += 2
                    continue
                if (s[i+1] == "C"):
                    number += 90
                    i += 2
                    continue
                else:
                    number += 10
                    i += 1
                    continue

            elif s[i] == "C":
                if (s[i+1] == "D"):
                    number += 400
                    i += 2
                    continue
                if (s[i+1] == "M"):
                    number += 900
                    i += 2
                    continue
                else:
                    number += 100
                    i += 1
                    continue

        return number



