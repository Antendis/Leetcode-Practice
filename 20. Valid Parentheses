class Solution(object):
    def isValid(self, s):
        
        openP = {"(": 0, "{": 1, "[": 2}
        closeP = {")": 0, "}": 1, "]": 2}
        i = 0

        while i < len(s):
            if openP.get(s[i]) != closeP.get(s[i+1]):
                return False
            i += 2
        return True
