class Solution(object):
    def isValid(self, s):
        
        curved, c1 = {"(": 0, ")": 1}, 0
        square, c2 = {"[": 0, "]": 1}, 0
        curly, c3 = {"{": 0, "}": 1}, 0
        i = 0

        while i < len(s):

            if s[i] == curved[0]:
                c1 += 1
            if s[i] == square[0]:
                c2 += 1
            if s[i] == curly[0]:
                c3 += 1

            if s[i] == curved[1]:
                c1 -= 1
                if c1 < 0: return false
            if s[i] == square[1]:
                c2 -= 1
                if c2 < 0: return false
            if s[i] == curly[1]:
                c3 -= 1
                if c3 < 0: return false

            i += 1

        return True
