class Solution(object):
    def lengthOfLastWord(self, s):
        
        backwards = s[::-1]
        counter = 0
        i = 0
        length = 0
        print(backwards)


        while i < counter:

            if backwards[i] != " ":
                length += 1

                if backwards[i + 1] == " ":

                    return length

            i += 1

        return length
