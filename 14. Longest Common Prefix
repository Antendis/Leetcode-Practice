class Solution(object):
    def longestCommonPrefix(self, strs):
        answer = ""
        i = 0
        n = 0

        while n != 99:
            temp = []
            for x in strs:
                temp.append(x[i])

            if (len(set(temp)) == 1): # if no duplicates
                answer += x[i]
                i += 1

            if (len(set(temp)) != 1):
                n = 99

        return answer
