
# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def mergeTwoLists(self, list1, list2):
        
        i = 0
        merged = []
        listsSize = 6
        firstList = list1.val
        secondList = list2.val

        while i < listsSize:

            if firstList < secondList:
                merged.append(firstList)
                if list1 is not None:
                    list1 = list1.next
                    if list1 is not None:
                        firstList = list1.val
            else:
                merged.append(secondList)
                if list2 is not None:
                    list2 = list2.next
                    if list2 is not None:
                        secondList = list2.val

            i += 1
        
        print(merged)



